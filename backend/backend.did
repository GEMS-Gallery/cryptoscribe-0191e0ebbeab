type Time = int;
type Result = 
 variant {
   err: text;
   ok: Post;
 };
type Post = 
 record {
   author: text;
   body: text;
   id: nat;
   timestamp: Time;
   title: text;
 };
service : {
  createPost: (text, text, text) -> (Result);
  getPost: (nat) -> (Result) query;
  getPosts: () -> (vec Post) query;
}
